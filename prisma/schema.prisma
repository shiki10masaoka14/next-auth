datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongodb"]
}

model Todo {
  id         String  @id @default(dbgenerated()) @map("_id") @db.ObjectId
  todo       String
  isComplete Boolean @default(false)
  disc       String?
}
